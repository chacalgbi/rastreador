<%= render "page_header" do %>
  <%= render "page_header_breadcrumb", title: "Editing push notification" do %>
  <li class="breadcrumb-item"><%= link_to "Admin", admin_path %></li>
  <li class="breadcrumb-item"><%= link_to "Push notifications", admin_push_notifications_path %></li>
  <li class="breadcrumb-item"><%= link_to "Show this push notification", [:admin, @push_notification] %></li>
  <% end %>
<% end %>

<div class="page-body">
  <div class="container-xl">
    <div class="card">
      <div class="card-body">
        <%= render "form", push_notification: @push_notification %>
      </div>
    </div>
  </div>
</div>
